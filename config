#!/bin/bash

export PACKAGE_CORE_NAME="yao"
export PACKAGE_CORE_BIN_NAME="$PACKAGE_CORE_NAME"
export PACKAGE_CORE_VERSION=v0.10.2
export PACKAGE_CORE_ORG="YaoApp"

export PACKAGE_CORE_ORG_DOREAMON_DESIGN="doreamon-design"
export PACKAGE_CORE_VERSION_DOREAMON_DESIGN=v0.10-1.2.0

if [ "$PLUGIN_PACKAGE_USE_DOREAMON_DESIGN" = "true" ]; then
  export PACKAGE_CORE_ORG=$PACKAGE_CORE_ORG_DOREAMON_DESIGN
  export PACKAGE_CORE_VERSION=$PACKAGE_CORE_VERSION_DOREAMON_DESIGN
fi

# export PACKAGE_CORE_RELEASE_DOWNLOAD=${PACKAGE_CORE_RELEASE_DOWNLOAD:-"https://github.com/${PACKAGE_CORE_ORG}/yao/releases/download"}
export PACKAGE_CORE_RELEASE_DOWNLOAD="https://github.com/${PACKAGE_CORE_ORG}/yao/releases/download"
